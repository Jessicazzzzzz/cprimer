//write a program that eight integers into an array and then prints them in reverse order


int main()
{  int int_arry[8];
   int i;
   printf("enter 8 digitals:");
   for (i=0;i<8;i++)
   {
       scanf("%d",&int_arry[i]);
   }
    for (i--;i>=0;i--)
    {
        printf("%5d",int_arry[i]);
    }
    return 0;
   }
   
